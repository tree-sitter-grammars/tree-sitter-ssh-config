{
  "name": "tree-sitter-ssh-config",
  "version": "0.3.0",
  "license": "MIT",
  "description": "OpenSSH config grammar for tree-sitter",
  "repository": "tree-sitter-grammars/tree-sitter-ssh-config",
  "main": "bindings/node",
  "types": "bindings/node",
  "author": {
    "name": "ObserverOfTime",
    "email": "chronobserver@disroot.org",
    "url": "https://observeroftime.github.io/"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "ssh"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "scripts": {
    "build": "tree-sitter generate --no-bindings",
    "install": "node-gyp-build",
    "parse": "tree-sitter parse",
    "prebuildify": "prebuildify --napi --strip",
    "test": "tree-sitter test"
  },
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "~0.21.0",
    "prebuildify": "^6.0.0"
  },
  "tree-sitter": [
    {
      "scope": "text.ssh_config",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "content-regex": "^\\s*IdentityFile",
      "file-types": [
        "ssh_config"
      ]
    }
  ]
}
